{
  "name": "@dcrgll/dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "pnpm with-env next dev --port 8080",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@dcrgll/auth": "^0.1.0",
    "@dcrgll/ui": "^0.1.0",
    "@t3-oss/env-nextjs": "^0.6.1",
    "next": "^13.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.13.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@dcrgll/eslint-config": "^0.2.0",
    "@dcrgll/prettier-config": "^0.1.0",
    "@dcrgll/tailwind-config": "^0.1.0",
    "@dcrgll/tsconfig": "^0.1.0",
    "@types/node": "^18.17.18",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.49.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "tailwindcss": "3.3.2",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@dcrgll/eslint-config/base",
      "@dcrgll/eslint-config/nextjs",
      "@dcrgll/eslint-config/react"
    ]
  },
  "prettier": "@dcrgll/prettier-config"
}
